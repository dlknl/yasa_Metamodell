<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="LEGOSAR.model.legosar.presentation.LegosarModelWizardID"
            name="%_UI_LegosarModelWizard_label"
            class="LEGOSAR.model.legosar.presentation.LegosarModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/LegosarModelFile.gif">
         <description>%_UI_LegosarModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="LEGOSAR.model.legosar.presentation.LegosarEditorID"
            name="%_UI_LegosarEditor_label"
            icon="icons/full/obj16/LegosarModelFile.gif"
            extensions="legosar"
            class="LEGOSAR.model.legosar.presentation.LegosarEditor"
            contributorClass="LEGOSAR.model.legosar.presentation.LegosarActionBarContributor">
      </editor>
   </extension>

</plugin>
